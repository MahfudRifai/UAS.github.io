                                    'type' : data.type
                                }
                                iframe_src.postMessage(message, "*");
                            },
                            function() {
                                var message = {
                                    'status' : 'error',
                                    'longitude' : '',
                                    'latitude' : '',
                                    'type' : data.type
                                }
                                iframe_src.postMessage(message, "*");
                                console.log('Unable to retrieve your location');
                            }
                        );
                    }
                });
            }
        }
    }
    catch(err)
    {}
}, false);
